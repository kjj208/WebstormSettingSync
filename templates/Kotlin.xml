  <template name="closure" value="{$PARAM$ -&gt; $PARAM_COPY$}" description="闭包(没有名称的函数)" toReformat="true" toShortenFQNames="true">
    <variable name="PARAM" expression="" defaultValue="&quot;x&quot;" alwaysStopAt="true" />
